<Window x:Class="Tren3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tren3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        MinHeight="450" MinWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="72"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <DockPanel Background="#FF424242">
            <DockPanel HorizontalAlignment="Left">
                <Button Style="{StaticResource ButtonNavigate}" Content="Склады"/>
                <Button Style="{StaticResource ButtonNavigate}" Content="Материалы"/>
            </DockPanel>
        </DockPanel>
        <Frame Grid.Row="1" x:Name="MainFrame"/>
    </Grid>
</Window>
